{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": "google",
  "rules": {
    "indent": [2, 2, {"VariableDeclarator": { "var": 2, "let": 2, "const": 3} }],
    "one-var": 0,
    "no-spaced-func": 0,
    "require-jsdoc": 0,
    "space-before-function-paren": [2, {"anonymous": "always", "named": "always"}]
  }
}
